{
  "name": "social-sole",
  "version": "1.0.0",
  "description": "A Social Media site for sneakerheads",
  "main": "server.js",
  "engines": {
    "node": "10.18.1",
    "npm": "6.13.7"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Marcus Mckinstry",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "firebase": "^5.8.2",
    "gravatar": "^1.6.0",
    "icomoon-react": "^2.0.16",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.23.0",
    "mongoose": "^5.2.17",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-moment": "^0.8.4",
    "react-quill": "^1.3.3",
    "react-scripts": "3.0.0",
    "react-trumbowyg": "^1.1.0",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
